{
  "extends": [
    "wesbos"
  ],
  "rules": {
    "no-plusplus": 0,
    "no-console": 0,
    "quotes": [
      "error",
      "double"
    ],
    "react/destructuring-assignment": 0,
    "react/prop-types": 0,
    "react/state-in-constructor": 0,
    "react/no-access-state-in-setstate": 0,
    "react/no-unused-state": 0,
    "lines-between-class-members": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "doubleQuote": true,
        "printWidth": 120,
        "tabWidth": 4
      }
    ]
  }
}